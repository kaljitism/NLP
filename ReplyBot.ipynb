   "source": [
    "def swap_pronoun():\n",
    "    phrase = input(\"USER: \")\n",
    "    if 'I' in phrase:\n",
    "        return re.sub('I', 'you', phrase)\n",
    "    if 'my' in phrase:\n",
    "        return re.sub('my', 'your', phrase)\n",
    "    else:\n",
    "        return phrase"
   ]
